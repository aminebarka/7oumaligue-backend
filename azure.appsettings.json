{
  "AzureSettings": {
    "RequiredPort": 8080,
    "AllowedHosts": "*",
    "NodeVersion": "22.15.0",
    "Environment": "production",
    "DeploymentType": "GitHubActions"
  },
  "NetworkSettings": {
    "ListenHost": "0.0.0.0",
    "ProductionPort": 8080,
    "DevelopmentPort": 5000,
    "HealthCheckPath": "/health"
  },
  "SecuritySettings": {
    "CORS": {
      "AllowedOrigins": [
        "https://7oumaligue.azurestaticapps.net",
        "http://localhost:5173"
      ],
      "AllowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"]
    },
    "RateLimiting": {
      "Enabled": true,
      "WindowMs": 900000,
      "MaxRequests": 1000
    }
  },
  "DatabaseSettings": {
    "ConnectionTimeout": 30000,
    "MaxConnections": 10,
    "SSL": true
  },
  "LoggingSettings": {
    "Level": "info",
    "Format": "json",
    "Output": "console"
  }
} 